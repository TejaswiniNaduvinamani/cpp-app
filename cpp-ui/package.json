{
  "name": "cpp-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve-proxy": "ng serve --proxy-config proxy.local.json",
    "json-server": "node test/json-server/server.js",
    "start-with-jetty": "ng serve --proxy-config proxy.jetty.json",
    "start-with-proxy": "concurrently --kill-others \"npm run json-server\" \"npm run serve-proxy\"",
    "build": "ng build",
    "build-prod": "ng build --prod --base-href /cpp/",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage --browsers PhantomJS",
    "test-coverage-local": "ng test --code-coverage --browsers Chrome --no-single-run",
    "lint": "ng lint",
    "webdriver-update": "webdriver-manager update",
    "e2e": "protractor ./test/protractor.conf.js",
    "e2eci": "concurrently --raw  --kill-others --success first \"ng e2e --proxy-config proxy.local.json --port 8192 --config test/protractor-ci.conf.js --cucumberOpts.tags=\"@distribution-centers-selection\" -host=buildcrmbo.gfs.com\" \"npm run json-server\" "
  },
  "private": true,
